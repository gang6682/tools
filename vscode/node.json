	"rcm": {
		"scope": "javascript,typescript,typescriptreact,javascriptreact",
		"prefix": "rcm",
		"body": [
			"import type { Effect, Reducer } from 'umi';",
			"",
			"export type NAMEState = {",
			"\tcurrentNAME?: any;",
			"};",
			"",
			"export type NAMEType = {",
			"\tnamespace: '$TM_FILENAME_BASE';",
			"\tstate: NAMEState;",
			"\teffects: {",
			"\t\tfetchNAME: Effect;",
			"\t};",
			"\treducers: {",
			"\t\tsaveNAME: Reducer<NAMEState>;",
			"\t};",
			"};",
			"",
			"const $TM_FILENAME_BASE: NAMEType = {",
			"\tnamespace: '$TM_FILENAME_BASE',",
			"\t",
			"\tstate: {",
			"\t\tcurrentNAME: {},",
			"\t},",
			"\t",
			"\teffects: {",
			"\t\t*fetchNAME({ payload }, { put }) {",
			"\t\t\tyield put({",
			"\t\t\t\ttype: 'saveNAME',",
			"\t\t\t\tpayload,",
			"\t\t\t});",
			"\t\t},",
			"\t},",
			"\t",
			"\treducers: {",
			"\t\tsaveNAME(state, { payload }) {",
			"\t\t\treturn {",
			"\t\t\t\t...state,",
			"\t\t\t\tcurrentNAME: payload || {},",
			"\t\t\t};",
			"\t\t},",
			"\t},",
			"};",
			"",
			"export default $TM_FILENAME_BASE;",
		],
		"description": "react models"
	},
